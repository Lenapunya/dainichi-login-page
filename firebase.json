{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    
"redirects": [
{
 "source": "/",
 "destination": "https://dainichi-portal.com",
 "type": 301
}
]

  }
}
